pipeline {

  agent any

  stages {

    stage('Checkout Source') {
      steps {
        git url:'https://hassanniy@bitbucket.org/hassanniy/demo.git'        
      }
    }

    stage('Deploy App') {
      steps {
        script {
          kubernetesDeploy(configs: "nginx.yaml")
        }
      }
    }

  }

}