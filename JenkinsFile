pipeline {

  agent any

  stages {

    stage('Checkout Source') {
      steps {
         git branch: 'main', credentialsId: '759d11e2-3343-4e34-bc5b-be332aaa996d', url: 'https://github.com/hassanniy/jenkins.git'
      }
    }

    stage('Deploy App') {
      steps {
        script {
          kubernetesDeploy(configs: "nginx.yaml")
        }
      }
    }

  }

}


